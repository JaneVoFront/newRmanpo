        .search-info
            a.search.search-info_items(href="#" title='Поиск')
                img.img-svg(src="/assets/img/search.svg")
            a.info.search-info_items(href="#" title='Обратная связь, контакты')
                i.fa.fa-question-circle-o(aria-hidden="true")

