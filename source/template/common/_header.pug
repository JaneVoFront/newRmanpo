header.header
    .header-top
        .header-top_item.col-2.left
            .header-top_date.date
                img.date__img(src="/assets/img/calendar.svg")
                span.date__text 18 Декабря, 2020, 04:57
            a.icon-home.header-top_item_icon-home(href="#" title='На главную')
                i.fa.fa-home(aria-hidden="true")
            a.icon-eye(href="#" title='Версия сайта для слабовидящих')
                            i.fa.fa-eye(aria-hidden="true")
        .header-top_item.col-2.right
            .nav-eios.dropdown
                 a(href="#" alt='Войти в ЭИОС' title='Войти в ЭИОС')
                     i.fa.fa-sign-in
                     span Войти в ЭИОС
                     i.fa.fa-angle-down
                 ul.nav-eios_menu
                     li
                        a(href="#" alt='На главную' title='Высшее образование') Высшее образование
                     li
                        a(href="#" alt='На главную' title='Дополнительное образование') Дополнительное образование
    .header-central
        nav.gen-menu
            ul
                li.gen-menu_item
                    a(href="#" alt='Об академии' title='Об академии') Об академии
                li.gen-menu_item
                    a(href="#" alt='Образование' title='Образование') Образование
                li.gen-menu_item.gen-menu_item__logo
                    .logo
                          a(href="#" alt='На главную' title='На главную')
                            img.logo_img(src="/assets/img/logo.png")
                            span.logo_text Российская медицинская академия <br> непрерывного профессионального образования
                li.gen-menu_item
                    a(href="#" title='Высшее образование') Наука
                li.gen-menu_item
                    a(href="#" title='Высшее образование') Клиника
